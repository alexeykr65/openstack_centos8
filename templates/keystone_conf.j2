[DEFAULT]
debug=False

[database]
connection = mysql+pymysql://keystone:{{KEYSTONE_DBPASS}}@{{controller.name}}/keystone

[credential]
key_repository=/etc/keystone/credential-keys

[fernet_tokens]
key_repository=/etc/keystone/fernet-keys

[oslo_policy]
policy_file=/etc/keystone/policy.json

[token]
expiration=86400
provider=fernet
revoke_by_id=True

